          "Uid": {
            "type": "string",
            "nullable": true
          },
          "Username": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SamuraiDTO": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "integer",
            "format": "int32"
          },
          "BusinessId": {
            "type": "string",
            "nullable": true
          },
          "Level": {
            "type": "integer",
            "format": "int32"
          },
          "Experience": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "StringInt32KeyValuePair": {
        "type": "object",
        "properties": {
          "Key": {
            "type": "string",
            "nullable": true
          },
          "Value": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
