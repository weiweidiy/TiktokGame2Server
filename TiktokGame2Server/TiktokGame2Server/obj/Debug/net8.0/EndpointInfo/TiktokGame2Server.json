              "$ref": "#/components/schemas/JCombatTurnBasedEvent"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LevelNodeDTO": {
        "type": "object",
        "properties": {
          "Id": {
            "type": "integer",
            "format": "int32"
          },
          "BusinessId": {
            "type": "string",
            "nullable": true
          },
          "Process": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PlayerDTO": {
        "type": "object",
